{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "./src/*"
            ],
            "@layout": [
                "./src/layout/"
            ],
            "@layout/*": [
                "./src/layout/*"
            ],
            "@assets/*": [
                "./src/assets/*"
            ],
            "@public/*": [
                "./public/*"
            ],
            "@lib": [
                "./src/lib/"
            ],
            "@shared": [
                "./src/shared/"
            ],
            "@views/*": [
                "./src/views/*"
            ],
            "@components": [
                "./src/components/"
            ],
            "@interface": [
                "./src/interface/"
            ],
            "@api": [
                "./src/api/"
            ],
            "@api/*": [
                "./src/api/*"
            ]
        },
    },
}